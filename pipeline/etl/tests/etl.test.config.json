{
    "version":"1.0",
    "downloads": {
        "base_directory":"etl/tests/downloads",
        "sources_filepath":"etl/tests/etl.tests.sources.json",
        "manifest_directory": "etl/tests/manifests",
        "strategies": [
            {
                "mime_type": "application/json",
                "handler": "download_json"
            },
            {
                "mime_type":"application/gzip",
                "handler":"download_binary"
            },
            {
                "mime_type":"application/octet-stream",
                "handler":"download_binary"
            },
            {
                "mime_type":"default",
                "handler":"download_binary"
            }
        ],
        "max_chunk_size": 16777216,
        "timestamp_format": "%Y%m%d_%H%M%S"
    },
    "environment": "dev"
}